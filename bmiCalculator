//72319

#include <iostream>

int main() {
  
  double height;
  double weight;
  double bmi;
  
  // Ask user for their height
  
  std::cout << "Enter your height (m): ";
  std::cin >> height;
  
  // Now ask the user for their weight and calculate BMI
  
  std::cout << "\nEnter your weight (kg): ";
  std::cin >> weight;
  
  bmi = weight/(height * height);
  
  std::cout << "\nYour BMI is " << bmi << "\n\nThank you for providing your information!\n";
  
  return 0;

}
